selec() {
echo ""
echo ""
echo -e "\e[1;36m┏━━\e[1;35mDarken\e[1;36m-\e[1;35mPasiya\e[1;36m━\e[1;33m▶"
echo -e "\e[1;36m┃"
read -p $'\e[1;36m┗━━\e[1;32mContact\e[1;36m━━━\e[1;32mMe\e[1;36m━━\e[1;33m▶ ' chose
if [[ $chose = 1 || $chose = 01 ]]; then
Wame
elif [[ $chose = 2 || $chose = 02 ]]; then
Direct
elif [[ $chose = 3 || $chose = 03 ]]; then
Group
elif [[ $chose = 4 || $chose = 04 ]]; then
Custom
elif [[ $chose = 5 || $chose = 05 ]]; then
Any
elif [[ $chose = 6 || $chose = 06 ]]; then
Temp
else
sleep 1
exit
fi
}
Wame() {
echo
echo
echo -e "\e[1;34mex : \e[1;33m+94761234567 \e[1;36m/ \e[1;33m94761234567"
read -p $'\e[1;35mEnter Your Number With Country Code : \e[1;36m' wame
sleep 1
echo
echo
echo -e "\033[1;34m┏━━━━━━━━━━\033[1;36mCOPY LINK\033[1;34m━━━━━━━━━━┓"
echo -e "┃"
echo -e "┣━┳━[\033[1;47;31m  Direct Link  \033[0m]\033[1;34m"
echo -e "┣━┻━▶  \033[1;32mwa.me/"$wame
echo -e "\033[1;34m┃"
echo -e "\e[1;34m┣━┳━[\033[1;47;31m  Web Link  \033[0m]\033[1;34m"
echo -e "┣━┻━▶  \033[1;32mhttps://wa.me/"$wame
