echo -e ""
echo
read -p $'\e[1;33mIf You Want Continue \e[1;36mY/N\e[1;37m : ' con
if [[ $con = y || $con = Y ]]; then
MENU
selec
else
echo
echo
sleep 1
exit
fi
}
Direct() {
