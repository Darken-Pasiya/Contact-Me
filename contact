Custom() {
echo
echo
url_checker() {
if [ ! "${1//:*}" = http ]; then
if [ ! "${1//:*}" = https ]; then
echo -e "\e[1;31m[\e[1;33m!\e[1;31m] Invalid URL. Please use \e[1;33mhttp \e[1;31mor \e[1;33mhttps.\e[0m"
exit 1
fi
fi
}
echo -e "\e[1;36mPaste Phishing URL Or Any URL here (\e[1;33mwith http or https\e[1;36m): "
read phish
url_checker $phish
MENU
sleep 1
echo
echo -e "           \e[1;32mProcessing and Modifing Your URL"
echo ""
short=$(curl -s https://is.gd/create.php\?format\=simple\&url\=${phish})
shorter=${short#https://}
echo -e "\n                  \e[1;33;44m ### Modifing ###\e[0m"
sleep 1
echo ""
echo -e "  Domain to mask the Phishing URL (\e[1;33mwith http or https\e[0m)"
echo -e " ex: \e[1;35mhttps://google.com\e[0m"
echo -e "     \e[1;35mhttp://Darken-Pasiya.com\e[0m"
echo -e "     \e[1;35mhttps://Ehi-Master"
echo ""
echo -en "\e[37m~/\e[1;32m$\e[1;37m@\e[1;32mroot\e[1;37m@\e[1;32mContact-Me\e[1;37m~>\e[1;36m "
read Cust
url_checker $Cust
MENU
sleep 0.5
echo -e '\n\e[1;37mType social engineering words:( \e[1;33mfree-money, best-pubg-tricks\e[0m)'
echo -e "\e[1;36mDon't use space just use '\e[1;33m-\e[1;36m' between social engineering words\e[0m"
echo
echo -en "\e[37m~/\e[1;32m$\e[1;37m@\e[1;32mroot\e[1;37m@\e[1;32mContact-Me\e[1;37m~>\e[36m "
read words
if [[ -z "$words" ]]; then
echo -e "\e[31m[!] No words.\e[0m"
echo -e "\nGenerating Custom Domain Link...\n"
final=$Cust@$shorter
sleep 1
echo -e "\e[1;33mYour Custom Domain URL:\e[36m ${final} \e[0m\n"
exit
fi
if [[ "$words" =~ " " ]]; then
echo -e "\e[31m[!] Invalid words. Please avoid space.\e[0m"
echo -e "\n\e[1;32mGenerating Custom Domain Link...\n"
final=$Cust@$shorter
sleep 1
echo -e "\e[1;33mYour Custom Domain URL:\e[36m ${final} \e[0m\n"
exit
fi
echo -e "\n\e[1;32mGenerating Custom Domain Link...\n"
sleep 2
echo ""
final=$Cust-$words@$shorter
sleep 1
echo -e "\e[1;33mYour Custom Domain URL:\e[36m ${final} \e[0m\n"
echo
}
